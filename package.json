  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^2.1.0",
    "fetch-mock": "^7.3.1",
    "jest-cli": "^24.4.0",
    "jwt-decode": "^2.2.0",
    "moxios": "^0.4.0",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
